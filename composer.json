{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "2captcha/2captcha": "^1.2",
        "barryvdh/laravel-translation-manager": "^0.6.7",
        "bigdevwhale/godaddy-api-wrapper": "^1.0",
        "blade-ui-kit/blade-heroicons": "^1.2",
        "guzzlehttp/guzzle": "^7.9",
        "illuminate/view": "*",
        "io-developer/php-whois": "^4.1",
        "irazasyed/telegram-bot-sdk": "^3.14",
        "jaybizzle/crawler-detect": "^1.3",
        "jenssegers/mongodb": "*",
        "kaleidpixel/domain-expiry-checker": "^0.0.1",
        "laracasts/flash": "^3.2",
        "laravel-notification-channels/telegram": "^5.0",
        "laravel/breeze": "^2.2",
        "laravel/framework": "^11.31",
        "laravel/horizon": "^5.29",
        "laravel/pulse": "^1.3",
        "laravel/reverb": "^1.0",
        "laravel/scout": "^10.11",
        "laravel/telescope": "^5.2",
        "laravel/tinker": "^2.10",
        "league/flysystem-aws-s3-v3": "^3.29",
        "league/flysystem-sftp-v3": "^3.29",
        "mobiledetect/mobiledetectlib": "^4.8",
        "mongodb/laravel-mongodb": "^5.1",
        "mongodb/mongodb": "^1.20",
        "mtownsend/response-xml": "^2.3",
        "nelexa/zip": "^4.0",
        "nesbot/carbon": "^3.8",
        "php-webdriver/webdriver": "^1.15",
        "predis/predis": "^2.3",
        "spatie/browsershot": "^5.0",
        "spatie/image": "^3.7",
        "spatie/laravel-html": "^3.11",
        "spatie/laravel-permission": "^6.16",
        "spatie/ssh": "^1.13",
        "symfony/css-selector": "^7.2",
        "symfony/dom-crawler": "^7.2",
        "symfony/filesystem": "^7.2",
        "symfony/finder": "^7.2",
        "symfony/process": "^7.2",
        "tanmuhittin/laravel-google-translate": "^2.4",
        "ua-parser/uap-php": "^3.9",
        "yajra/laravel-datatables": "^11.0",
        "yaza/laravel-repository-service": "^5.1",
        "zanysoft/laravel-zip": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "beyondcode/laravel-dump-server": "^2.0",
        "fakerphp/faker": "^1.23",
        "filp/whoops": "^2.16",
        "kamona/kui-laravel-breeze": "^0.5.1",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.5",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
